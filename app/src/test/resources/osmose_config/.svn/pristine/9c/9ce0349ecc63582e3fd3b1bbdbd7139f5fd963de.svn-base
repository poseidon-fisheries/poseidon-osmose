# OSM main configuration file
osmose.version;Osmose 3 Update 2 (2015/03/01)

# Time
simulation.time.ndtPerYear;24
simulation.time.nyear;50

# Restart
simulation.restart.file;null

# Advanced parameters
simulation.nschool;40
simulation.ncpu;1
simulation.nplankton;4
simulation.nsimulation;1
simulation.nspecies;10

# Mortality algorithm
mortality.algorithm;stochastic
mortality.subdt;10

# Configuration files
osmose.configuration.output;osm_param-output.csv
osmose.configuration.movement;osm_param-movement.csv
osmose.configuration.mpa;osm_param-mpa.csv
osmose.configuration.mortality.fishing;osm_param-fishing.csv
osmose.configuration.mortality.natural;osm_param-natural-mortality.csv
osmose.configuration.mortality.predation;osm_param-predation.csv
osmose.configuration.mortality.starvation;osm_param-starvation.csv
osmose.configuration.reproduction;osm_param-reproduction.csv
osmose.configuration.species;osm_param-species.csv
osmose.configuration.plankton;osm_param-ltl.csv
osmose.configuration.grid;osm_param-grid.csv
osmose.configuration.initialization;osm_param-seeding.csv

